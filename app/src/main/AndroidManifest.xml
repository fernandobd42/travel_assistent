<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fernandolucasgontijo.travel_assistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/ACTIVITY_MAIN_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pages.RegisterCar"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="pages.PlanTravel"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="pages.PlannedTravel"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="pages.ListPlannedTravel"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="pages.TravelHelper"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="pages.ListRefuel"
            android:theme="@style/AppTheme"></activity>
</application>

</manifest>